    "@acme/tsconfig": "workspace:*",
    "@babel/core": "^7.25.2",
    "@types/react": "catalog:react19",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.9",
    "react-test-renderer": "19.0.0",
    "typescript": "catalog:"
  }
